	removeReviewer = group user/David Sariel (dsariel)
	submit = group user/David Sariel (dsariel)
[access "refs/heads/*"]
	create = group user/David Sariel (dsariel)
	push = +force group user/David Sariel (dsariel)
	pushMerge = group user/David Sariel (dsariel)
	read = group user/David Sariel (dsariel)
[access "refs/tags/*"]
	push = group user/David Sariel (dsariel)
	push = +force group user/David Sariel (dsariel)
